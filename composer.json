{
    "name": "sixty-nine/php-cloud-lib",
    "description": "A library to generate words clouds in PHP",
    "type": "library",
    "keywords": ["cloud"],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniele Barsotti",
            "email": "github@sixty-nine.ch"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "SixtyNine\\Cloud\\": "src/SixtyNine/Cloud"
        }
    },
    "require": {
        "php": ">=8.1.0",
        "doctrine/collections": "^1.4",
        "jms/serializer": "^1.2",
        "imagine/imagine": "~0.5.0",
        "symfony/console": "~6.2.8",
        "symfony/filesystem": "^6.2.10",
        "monolog/monolog": "^3.3.1",
        "webmozart/assert": "^1.11.0",
        "symfony/stopwatch": "^6.2.7",
        "symfony/yaml": "^6.2.7",
        "sixty-nine/php-data-types": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1.2"
    },
    "config": {
        "bin-dir": "bin"
    }
}
